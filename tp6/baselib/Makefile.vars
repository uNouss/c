## Modify these settings
PREFIX=arm-none-eabi
GCC_VERSION=

## System commands definitions
CC=$(PREFIX)-gcc$(GCC_VERSION)
LD=$(CC)
AR=$(PREFIX)-ar
AS=$(PREFIX)-as
OBJCOPY=$(PREFIX)-objcopy
OBJDUMP=$(PREFIX)-objdump
NM=$(PREFIX)-nm
SIZE=$(PREFIX)-size

## Platform configuration
EEPROM_START=0x0
EEPROM_END=0x80000
RAM1_START=0x1000000
RAM1_END=0x10008000
RAM2_START=0x2007C000
RAM2_END=0x20084000

STACK_END=$(RAM1_END)
INIT_VEC=$(EEPROM_START)

